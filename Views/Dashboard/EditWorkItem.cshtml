    function AddNewWorkItem(){
         var id = @projectId;
         var workItemTypeId = @WorkItemType;
         //console.log($("#ItemReason :selected").val()); // The value of the selected option
         ShowLoader();

          var dto = {
            "WorkItemId" : @WorkItemData.Id,
            "WorktItemType" : workItemTypeId,
            "Title": $("#Title").val(),
            "ProjectId" : id,
            "AssignedUser" :assignedUser,
            "Description" : $("#Description").val(),
            "AcceptanceCriteria": $("#AcceptanceCriteria").val(),
            "State" :  ItemState.value,
            "ItemReason" :  ItemReason.value,
            "ItemArea" : ItemArea.value,
            "ItemIteration" : ItemIteration.value,
            "ItemPriority" : ItemPriority.value,
            "ItemSeverity" : ItemSeverity.value,
            "ItemActivity" : ItemActivity.value,
            "FoundInBuild" :ItemFoundBuild.value,
            "ResolvedInBuild" : ItemResolvedBuild.value,
            "RepoSteps" : $("#RepoSteps").val(),
            "SystemInfo" : $("#SystemInfo").val(),
            "OriginalEstimate" : $("#OriginalEstimate").val(),
            "ItemRemaining" : $("#ItemRemaining").val(),
            "ItemCompleated" : $("#ItemCompleated").val(),
            "Effort" :  $("#Effort").val(),
            "RiskId" : $("#Risk :selected").val(),
            "BusinessValue":  $("#BValue").val(),
            "TimeCriticality" : $("#TimeCriticality").val(),
            "StartDate" : startDate,
            "EndDate" : endDate,
            "ValueAreId" :  ValueArea.value,
            "DueDate" : dueDateVal,
            "Rank" :$("#Rank").val() ,
            "StoryPoints" : $("#StoryPoints").val(),
            "SelectedChildren" : selectedChildren,
            "SelectedFiles": selectedFilesTransfer,
            "SystemDesignInfo": systemModel

        }
        if($("#Description").val() !== undefined)
            dto.Description = $("#Description").children()[0].innerHTML;
        if($("#AcceptanceCriteria").val() !== undefined)
            dto.AcceptanceCriteria = $("#AcceptanceCriteria").children()[0].innerHTML;
        if($("#RepoSteps").val() !== undefined)
            dto.RepoSteps =  $("#RepoSteps").children()[0].innerHTML;
        if($("#SystemInfo").val() !== undefined)
            dto.SystemInfo = $("#SystemInfo").children()[0].innerHTML;
        //console.log(dto);
        if($("#Title").val()) {
            $.ajax({
                type: 'POST',
                url: '/Dashboard/UpdateWorkItem',
                data: JSON.stringify(dto),
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (response) {
                    window.location=document.referrer;
                   // window.location.href = "@ReturnPath";
                },
                error: function (xhr, status, error) {
                    console.log(error);
                }
            });
        }
        else
        {
            alert("Please fill in the title of the current work item!");
        }
    }

